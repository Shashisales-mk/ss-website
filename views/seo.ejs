<script>
    const seoItems = document.querySelectorAll('.seo-hed-in li a');

    const removeActiveClass = () => {
        seoItems.forEach(item => item.classList.remove('seo-active'));
    };

    const observerCallback = (entries) => {
        entries.forEach(entry => {
            const id = entry.target.getAttribute('id');
            const correspondingLink = document.querySelector(`.seo-hed-in li a[href="#${id}"]`);

            if (entry.isIntersecting) {
                removeActiveClass();
                correspondingLink.classList.add('seo-active');
            }
        });
    };

    const observerOptions = {
        root: null,
        threshold: 0.5
    };

    const observer = new IntersectionObserver(observerCallback, observerOptions);

    const sections = document.querySelectorAll('#seo-services, #our-technology, #case-studies, #seo-pricing, #our-approach');

    sections.forEach(section => observer.observe(section));
</script>

<script>
    function animateNumber(element, target) {
        let start = 0;
        let end = parseInt(target);
        let duration = 2000; 
        let increment = end / (duration / 30); 
        let current = start;
        let interval = setInterval(function() {
            current += increment;
            if (current >= end) {
                current = end;
                clearInterval(interval);
            }
            element.innerHTML = Math.floor(current) + "%";
        }, 30);
    }
    function handleIntersection(entries, observerr) {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                let el = entry.target;
                let target = el.getAttribute('data-target');
                el.style.opacity = 1;
                animateNumber(el, target);
                observerr.unobserve(el);
            }
        });
    }
    const observerr = new IntersectionObserver(handleIntersection, {
        root: null,
        threshold: 0.5
    });
    document.querySelectorAll('.scre').forEach(el => {
        observerr.observe(el);
    });
</script>


