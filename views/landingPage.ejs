        <script>
            // Save scroll position before page unload
            window.addEventListener('beforeunload', function () {
                sessionStorage.setItem('scrollPosition', window.scrollY);
            });

            // Restore scroll position on load
            window.addEventListener('load', function () {
                if (sessionStorage.getItem('scrollPosition')) {
                    window.scrollTo(0, parseInt(sessionStorage.getItem('scrollPosition')));
                }
                // Initialize GSAP and ScrollTrigger after restoring scroll
                initAnimations();
            });

            document.addEventListener('DOMContentLoaded', function () {
                gsap.registerPlugin(ScrollTrigger);

                // Disable ScrollTrigger's automatic refresh
                ScrollTrigger.config({ autoRefreshEvents: "visibilitychange,DOMContentLoaded,load" });

                const container = document.querySelector('.brand-container');
                const cards = gsap.utils.toArray('.brand-card');

                // Horizontal scroll animation for the cards
                let tl = gsap.timeline({
                    scrollTrigger: {
                        trigger: "#whyChooseUs",
                        pin: true,
                        pinSpacing: true,
                        start: "top top",
                        end: () => "+=" + (container.offsetWidth * (cards.length - 1)),
                        scrub: 1,
                        onUpdate: self => {
                            if (self.progress === 1 && self.direction > 0) {
                                const nextSection = document.querySelector('#whyChooseUs').nextElementSibling;
                                if (nextSection) {
                                    ScrollTrigger.create({
                                        trigger: nextSection,
                                        start: "top bottom",
                                        onEnter: () => nextSection.scrollIntoView({ behavior: 'smooth' })
                                    });
                                }
                            }
                        }
                    }
                });

                // Add each card to the timeline
                cards.forEach((card, i) => {
                    if (i > 0) {
                        tl.to(cards, {
                            xPercent: -113 * i,
                            duration: 1,
                            ease: "none"
                        });
                    }
                });

                // Rotate illustrations
                gsap.utils.toArray('.illustration').forEach(illustration => {
                    gsap.to(illustration, {
                        rotation: 360,
                        duration: 20,
                        repeat: -1,
                        ease: "none"
                    });
                });

                // Animate progress bars
                cards.forEach((card, index) => {
                    const progress = card.querySelector('.progress');
                    gsap.to(progress, {
                        width: "100%",
                        scrollTrigger: {
                            trigger: card,
                            containerAnimation: ScrollTrigger.getById("whyChooseUs"),
                            start: "left center",
                            end: "right center",
                            scrub: true
                        }
                    });
                });

                // Force ScrollTrigger to recalculate all scrollbar-related values
                ScrollTrigger.refresh();



            });
        </script>


